<?xml version="1.0" encoding="UTF-8"?>
<plist version="1.0">
<dict>
	<key>name</key>
	<string>Comments</string>

	<key>scope</key>
	<string>text.wxml</string>

	<key>settings</key>
	<dict>
		<key>shellVariables</key>
		<array>
			<!-- 使用注释快捷键：定义注释的开头 -->
			<dict>
				<key>name</key>
				<string>TM_COMMENT_START</string>
				<key>value</key>
				<string><![CDATA[<!-- ]]></string>
			</dict>
			<!-- 使用注释快捷键：定义注释的结尾 -->
			<dict>
				<key>name</key>
				<string>TM_COMMENT_END</string>
				<key>value</key>
				<string><![CDATA[ -->]]></string>
			</dict>
		</array>
	</dict>
</dict>
</plist>
